{
    "compilerOptions": {
		"module": "esnext",
		"target": "es2017",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noEmitHelpers": true,
		"noEmitOnError": true,
		"skipLibCheck": true,
		"lib": [
			"es6",
			"dom",
			"es2015.iterable",
			"es2017"
		],
		"moduleResolution": "node",
		"removeComments": false
	},
    "exclude": [
        "node_modules"
    ],
    "compileOnSave": false,
	"files": [
        "./references.d.ts"
      ],    
}
